
<!-- <p id='error'><%= flash[:error] %></p> -->
<div id="new_game">
	<%= form_for @game do |f| %>
		
		<%= hidden_field(:game, :user_id, options = {value: "#{current_user.id}"}) %>
		<%= f.label :name %>
		<%= f.text_field :name %>

		<%= f.label :theme %>
		<%= f.text_field :theme %>
		<%= f.submit %>
	<% end %>
</div>

